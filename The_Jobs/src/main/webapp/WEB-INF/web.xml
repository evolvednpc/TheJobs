<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>newpackage.RegisterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>newpackage.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>newpackage.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddApplicantServlet</servlet-name>
        <servlet-class>com.applicant.project.AddApplicantServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditApplicantServlet</servlet-name>
        <servlet-class>com.applicant.project.EditApplicantServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteApplicantServlet</servlet-name>
        <servlet-class>com.applicant.project.DeleteApplicantServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddAppointmentServlet</servlet-name>
        <servlet-class>com.appointment.project.AddAppointmentServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditAppointmentServlet</servlet-name>
        <servlet-class>com.appointment.project.EditAppointmentServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteAppointmentServlet</servlet-name>
        <servlet-class>com.appointment.project.DeleteAppointmentServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/RegisterServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddApplicantServlet</servlet-name>
        <url-pattern>/AddApplicantServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditApplicantServlet</servlet-name>
        <url-pattern>/EditApplicantServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteApplicantServlet</servlet-name>
        <url-pattern>/DeleteApplicantServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddAppointmentServlet</servlet-name>
        <url-pattern>/AddAppointmentServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditAppointmentServlet</servlet-name>
        <url-pattern>/EditAppointmentServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteAppointmentServlet</servlet-name>
        <url-pattern>/DeleteAppointmentServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
